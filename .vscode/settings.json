{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/.hg/store/**": true,
        "**/.git/**": true,
        "**/node_modules/**": true,
        "**/.vscode/**": true
    },
    "chat.mcp.serverSampling": {
        "NextReleaseMCP/.vscode/mcp.json: nextrelease-mcp": {
            "allowedModels": [
                "copilot/auto",
                "copilot/claude-3.5-sonnet",
                "copilot/gemini-2.0-flash-001",
                "copilot/gpt-4.1",
                "copilot/gpt-4o",
                "copilot/gpt-5-mini",
                "copilot/o3-mini"
            ]
        }
    },
    "window.zoomLevel": 1
}
